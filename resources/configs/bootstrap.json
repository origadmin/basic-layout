{
  "service_name": "helloworld",
  "version": "v1.0.0",
  "crypto_type": "argon2",
  "server": {
    "gins": {
      "network": "tcp",
      "addr": "${gins_address:0.0.0.0:7000}",
      "timeout": {
        "seconds": 180
      },
      "shutdown_timeout": {
        "seconds": 180
      },
      "read_timeout": {
        "seconds": 180
      },
      "write_timeout": {
        "seconds": 180
      },
      "idle_timeout": {
        "seconds": 180
      }
    },
    "http": {
      "network": "tcp",
      "addr": "${http_address:0.0.0.0:8000}",
      "timeout": {
        "seconds": 180
      },
      "shutdown_timeout": {
        "seconds": 180
      },
      "read_timeout": {
        "seconds": 180
      },
      "write_timeout": {
        "seconds": 180
      },
      "idle_timeout": {
        "seconds": 180
      }
    },
    "grpc": {
      "network": "tcp",
      "addr": "${grpc_address:0.0.0.0:9000}",
      "timeout": {
        "seconds": 180
      },
      "shutdown_timeout": {
        "seconds": 180
      },
      "read_timeout": {
        "seconds": 180
      },
      "write_timeout": {
        "seconds": 180
      },
      "idle_timeout": {
        "seconds": 180
      }
    },
    "middleware": {
      "cors": {},
      "metrics": {
        "name": "metrics"
      },
      "traces": {
        "name": "traces"
      },
      "logger": {
        "name": "logger"
      }
    }
  },
  "data": {
    "database": {
      "driver": "mysql",
      "source": "dsn"
    },
    "redis": {
      "network": "tcp",
      "addr": "${redis_address:127.0.0.1:6379}",
      "read_timeout": {
        "seconds": 180
      },
      "write_timeout": {
        "seconds": 180
      }
    }
  },
  "settings": {
    "crypto_type": "argon2"
  },
  "discovery": {
    "type": "${discovery_type:consul}",
    "consul": {
      "address": "${consul_address:127.0.0.1:8500}"
    },
    "etcd": {
      "endpoints": "${etcd_address:127.0.0.1:2379}"
    }
  }
}
