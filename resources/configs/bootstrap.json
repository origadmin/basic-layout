{
  "service_name": "helloworld",
  "version": "v1.0.0",
  "crypto_type": "argon2",
  "server": {
    "http": {
      "network": "tcp",
      "addr": "${http_address}",
      "timeout": "180s",
      "shutdown_timeout": "180s",
      "read_timeout": "180s",
      "write_timeout": "180s",
      "idle_timeout": "180s"
    },
    "grpc": {
      "network": "tcp",
      "addr": "${grpc_address}",
      "timeout": "180s",
      "shutdown_timeout": "180s",
      "read_timeout": "180s",
      "write_timeout": "180s",
      "idle_timeout": "180s"
    },
    "gins": {
      "network": "tcp",
      "addr": "${gins_address}",
      "timeout": "180s",
      "shutdown_timeout": "180s",
      "read_timeout": "180s",
      "write_timeout": "180s",
      "idle_timeout": "180s"
    },
    "middleware": {
      "cors": {
        "allow_all_origins": true,
        "allow_origins": [
          "*"
        ],
        "allow_methods": [
          "POST",
          "GET",
          "OPTIONS",
          "PUT",
          "DELETE"
        ],
        "allow_headers": [
          "Accept",
          "Content-Type",
          "Content-Length",
          "Accept-Encoding",
          "X-CSRF-Token",
          "Authorization"
        ]
      },
      "metrics": {
        "name": "metrics"
      },
      "traces": {
        "name": "traces"
      },
      "logger": {
        "name": "logger"
      }
    }
  },
  "data": {
    "database": {
      "driver": "mysql",
      "source": "dsn"
    },
    "redis": {
      "network": "tcp",
      "addr": "192.168.28.42:6379",
      "read_timeout": "180s",
      "write_timeout": "180s"
    }
  },
  "discovery": {
    "type": "consul",
    "consul": {
      "address": "${consul_address}",
      "heart_beat": true,
      "health_check": true
    },
    "etcd": {
      "endpoints": "192.168.28.42:2379"
    }
  },
  "settings": {
    "crypto_type": "argon2"
  }
}